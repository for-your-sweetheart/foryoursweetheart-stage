.header{position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 9999;
    background: #fff;}
body {
	margin-top: 114px;
}
.ps-main{padding-bottom:15px;}
.ps-home-features{margin-bottom:0;}
.ajax-load-more-wrap .alm-btn-wrap {
    padding: 25px 0;
}
.ajax-load-more-wrap .alm-btn-wrap .alm-load-more-btn {
    padding: 6px 15px;
}
.ps-post__content li{font-size:18px;}
.ps-post--detail{margin-bottom:30px;}
body.logged-in.admin-bar .header{top:32px;}
.alm-listing.flexbox .flex-item.hasbg .ps-post--inside.dark,.alm-listing.flexbox .flex-item.hasbg .ps-post--inside{background:transparent !important;}
.search-main .alm-reveal .flex-item{margin-bottom:20px;}
@media (min-width: 992px){
	.ps-banner {
		position: relative;
		height: 700px;
		background-size: cover !important;
		background-position: center center !important;
	}
	.ps-banner img{display:none !important;}
	.ps-home-features{background-position: 50% 695px;}
}
@media (min-width: 1200px){
	.ps-home-features{background-position: 50% 640px;}	
}
@media (max-width: 991px){
	.ps-hero {min-height:400px;}
	.wpb_column.vc_column_container.vc_col-sm-3{height:auto;}
	.alm-listing.flexbox .flex-item{width:calc(50% - 10px);}
	.ps-home-features .ps-section__content {background-position: 53% -30px;}
	.fys-event+.ps-articles .alm-listing.flexbox .flex-item{width:calc(50% - 30px);}
	.ps-home-features{background-position: 50% 835px;}
	.ps-section__features .wpb_column.vc_column_container.vc_col-sm-3{height:auto;}
    .ps-footer__nav li {margin-right: 25px;}
}
@media (max-width: 767px){
	.ps-hero {min-height:300px;}
	.alm-listing.flexbox .flex-item{width:calc(100% - 10px);}
	.img-left-body{position:relative;}
	.img-left-body .vc_single_image-wrapper{height:auto !important;}
	.img-left-body img{position:static;width:100%;}
	.wpProQuiz_questionList .wpProQuiz_questionListItem{width:100%; }
	.wpProQuiz_questionList .wpProQuiz_questionListItem:nth-child(odd),.wpProQuiz_questionList .wpProQuiz_questionListItem:nth-child(even){padding:0 0 15px !important;}
	.ps-section__content .wpb_single_image .vc_figure{width:100px;}
	.wpb_column.vc_column_container.vc_col-sm-3{height:auto;    border: none;}
	.ps-home-features .ps-section__content { background: none; }
	.wcyd-li li{font-size:12px;}
	.wcyd-li li span{font-size:18px;}	
	.ps-home-features{background: none !important;}
	.wpb_single_image.wpb_content_element.vc_align_center{margin-bottom:20px;}
	.wpb_single_image.wpb_content_element.vc_align_center{height:auto;}
	.ps-section__features .wpb_column.vc_column_container.vc_col-sm-3{margin-bottom:50px;border:none;}
	.ps-section__features .wpb_text_column.wpb_content_element:last-child .wpb_wrapper p:last-child{font-size:16px;}
	.ps-home-features .ps-section__content{padding-top: 0;}
	.ps-home-features .ps-section__content .ps-block--notice{margin-bottom:0;}
	.ps-footer__nav li{margin: 0 5px 5px;}	
	.page-id-31 .ps-post__footer{padding-bottom:0px !important;}
    .ps-block--notice .ps-block__content {
        width: 100%;
        text-align: center;
    }
    .ps-home-features .ps-section__content {
        background: none !important;
    }
}

@media (max-width: 520px){
	.fys-event+.ps-articles .alm-listing.flexbox .flex-item{width:100%;}
	.fys-event+.ps-articles .alm-listing.flexbox .flex-item .ps-post__title a{text-align:center;}
	.vc_gitem-zone{height:400px;}
}
@media (max-width: 365px){
	.ps-footer__nav li a{font-size:10px;}
}
@media screen and (-ms-high-contrast: none), (-ms-high-contrast: active) {
	.wpb_single_image img{max-width:90px}
	.ps-home-features .ps-section__content .wpb_content_element {margin:0;}
}
@media (min-width: 1200px) and (-ms-high-contrast: none), (-ms-high-contrast: active){
	.ps-home-features{background-position: 53% 640px;}	
}