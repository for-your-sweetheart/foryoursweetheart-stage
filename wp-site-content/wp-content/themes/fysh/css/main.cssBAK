.sub-menu { position: absolute; top: 100%; left: 0; padding: 10px 0; min-width: 240px; z-index: 1000; background-color: #fff; -webkit-transition: all 0.4s ease; -moz-transition: all 0.4s ease; -o-transition: all 0.4s ease; transition: all 0.4s ease; }

.sub-menu > li { display: block; margin-right: 0; width: 100%; }

.sub-menu > li > a { display: inline-block; padding: 8px 15px; width: 100%; font-size: 13px; }

.sub-menu > li > a:hover { background-color: #e82731; color: #fff; }

.menu { display: inline-block; }

.menu::after { clear: both; content: ""; display: table; }

.menu > li { display: inline-block; position: relative; }

.menu > li > a { display: inline-block; padding: 10px 15px; font-family: "Open Sans", sans-serif; font-size: 17px; font-weight: 600; line-height: 20px; text-transform: uppercase; color: #0e5ba0; }

.menu > li > a:hover, .menu li.current-menu-item a { color: #e82731; }

.menu > li .sub-menu { visibility: hidden; opacity: 0; }

.menu > li:hover .sub-menu { visibility: visible; opacity: 1; }

@media (max-width: 1199px) { .menu { position: absolute; top: 100%; left: 0; width: 100%; z-index: 100; background-color: #fff; border-bottom: 1px solid #e4e4e4; display: none; }
  .menu > li { display: block; float: none; border-bottom: 1px solid #eee; }
  .menu > li > a { display: block; padding: 10px 15px; font-size: 14px; }
  .menu > li.menu-item-has-children > a { position: relative; }
  .menu > li.menu-item-has-children > a:before { position: absolute; top: 50%; -webkit-transform: translateY(-50%); -moz-transform: translateY(-50%); -ms-transform: translateY(-50%); -o-transform: translateY(-50%); transform: translateY(-50%); right: 15px; content: "\f107"; font-family: FontAwesome; }
  .menu .sub-menu { padding: 0; position: relative; -webkit-transition: all 0s ease; -moz-transition: all 0s ease; -o-transition: all 0s ease; transition: all 0s ease; }
  .menu .sub-menu > li { border-bottom: 1px solid #eee; }
  .menu .sub-menu > li > a { padding: 10px 15px; padding-left: 30px; }
  .menu .sub-menu > li:last-child { border-bottom: 0; } }

.navigation { position: relative; }

.navigation .navigation__top { float: right; }

.navigation .navigation__top::after { clear: both; content: ""; display: table; }

.navigation .ps-search-btn { float: right; display: inline-block; width: 40px; height: 40px; }

.navigation .ps-search-btn i { position: absolute; top: 50%; left: 50%; -webkit-transform: translate(-50%, -50%); -moz-transform: translate(-50%, -50%); -ms-transform: translate(-50%, -50%); -o-transform: translate(-50%, -50%); transform: translate(-50%, -50%); color: #adadad; }

.navigation .menu { display: block; float: right; text-align: right; width: 100%; }

@media (max-width: 1199px) { .navigation .menu-toggle { float: right; }
  .navigation .menu { display: none; } }

